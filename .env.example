# Copy to .env and edit values.
# This file is loaded by config.sh if present.

# --- Gemini Authentication ---
GEMINI_AUTH_MODE=auto
GEMINI_API_KEY=
GEMINI_GCP_PROJECT=
GEMINI_GCP_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=

# --- Gemini Model ---
GEMINI_MODEL=gemini-2.5-flash

# --- Pipeline Settings ---
MAX_REVIEW_ITERATIONS=2
AGENT_TIMEOUT=180
TASK_DEBOUNCE_SECONDS=0.5
TASK_STABLE_CHECKS=2
TASK_STABLE_INTERVAL=0.5
WATCH_POLL_INTERVAL=2

# --- Analyst / Discussion ---
ENABLE_ANALYST=true
ENABLE_DISCUSSION=false
DISCUSSION_ROUNDS=1
DISCUSSION_FILE=

# --- Task Queue ---
ENABLE_TASK_QUEUE=false
TASK_QUEUE_DIR=
TASK_QUEUE_PATTERN=*.md
TASK_QUEUE_PRIORITY_REGEX=^P([0-9])_
TASK_QUEUE_DEFAULT_PRIORITY=5
TASK_QUEUE_YAML_PRIORITY_KEY=priority
TASK_QUEUE_YAML_TITLE_KEY=title
TASK_QUEUE_YAML_OWNER_KEY=owner
TASK_QUEUE_YAML_DUE_KEY=due
TASK_QUEUE_OWNER_FILTER=
TASK_QUEUE_DUE_WARN_DAYS=0
TASK_QUEUE_OWNER_PRIORITY_BIAS=
TASK_QUEUE_DUE_FORMATS=%Y-%m-%d,%Y-%m-%d %H:%M
TASK_QUEUE_DUE_TZ=local
TASK_QUEUE_OWNER_SUBDIR=false
TASK_QUEUE_DUE_WARN_HOURS=0
TASK_QUEUE_OWNER_AUTO_DIR=true
TASK_QUEUE_DUE_LABEL_PREFIX=DUE_
TASK_QUEUE_OVERDUE_ACTION=warn
WEBHOOK_NOTIFY_OVERDUE=false
WEBHOOK_OVERDUE_URL=
TASK_QUEUE_OVERDUE_REQUEUE=false
TASK_QUEUE_INPROGRESS_DIR=
TASK_QUEUE_DONE_DIR=
TASK_QUEUE_FAILED_DIR=
TASK_QUEUE_REQUEUE_ON_FAILURE=false
TASK_QUEUE_RETRY_MAX=3
TASK_QUEUE_RETRY_BACKOFF_BASE=30

# --- Summary Notification ---
WEBHOOK_INCLUDE_SUMMARY=true
TASK_SUMMARY_MAX_CHARS=280

# --- Lock / History ---
SWARM_LOCK_FILE=
HISTORY_DIR=
KEEP_RUNS=20

# --- Pipeline Retry ---
PIPELINE_RETRY_COUNT=1
PIPELINE_RETRY_DELAY=3

# --- Redaction ---
REDACT_VALUES=
REDACT_REPLACEMENT=[REDACTED]

# --- Webhook ---
WEBHOOK_URL=
WEBHOOK_TIMEOUT=5
WEBHOOK_INCLUDE_TASK=false
WEBHOOK_TEMPLATE=generic
WEBHOOK_OWNER_MAP=

# --- Security ---
UMASK_VALUE=077
SECURE_FILES=true
TASK_SECRET_ALLOW=false
TASK_SECRET_REGEX=AIza[0-9A-Za-z\-_]{20,}|sk-[A-Za-z0-9]{20,}|ghp_[A-Za-z0-9]{30,}|AKIA[0-9A-Z]{16}|-----BEGIN PRIVATE KEY-----

# --- Operations ---
STATUS_FILE=
MAINTENANCE_MODE=false

# --- tmux Settings ---
SWARM_SESSION=gemini-agent-team
